{
  "name": "semantic-multi-cascader",
  "version": "0.1.4",
  "private": false,
  "description": "A multiple cascader component for semantic",
  "keywords": [
    "react",
    "component",
    "semantic ui",
    "data",
    "modulable",
    "dropdown",
    "multi dropdown",
    "semantic-ui",
    "multiple cascader",
    "cascader react",
    "cascader",
    "multiple cascader",
    "react-semantic-ui",
    "Multiple Selection",
    "Multiple Dropdown",
    "Semantic UI React"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "demo.png",
    "CHANGELOG.md",
    "LICENSE.md"
  ],
  "author": "Jamal Hassouni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamalhassouni/semantic-multi-cascader/issues"
  },
  "homepage": "https://github.com/jamalhassouni/semantic-multi-cascader/",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamalhassouni/semantic-multi-cascader.git"
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "react": "*",
    "semantic-ui-react": "^2.1.1"
  },
  "dependencies": {
    "unstated-next": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.1",
    "classnames": "^2.3.1",
    "concurrently": "^7.0.0",
    "jest": "^26.4.4",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "sass": "^1.49.7",
    "semantic-ui-react": "^2.1.1"
  },
  "scripts": {
    "dev": "PORT=4000 react-scripts start",
    "start": "concurrently \"npm run dev \" \"npm run watch-css\" ",
    "build": "NODE_ENV=production  && rm -rf dist && mkdir dist && npx babel src/Components --out-dir dist --copy-files",
    "build-css": "node-sass --include-path scss src/components/MultiCascader/index.scss src/components/MultiCascader/index.css --output-style compressed",
    "watch-css": "nodemon -e scss -x \"npm run build-css\" ",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
